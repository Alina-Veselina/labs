//Даны два ненулевых числа. Найти сумму, разность, произведение и
//частное их квадратов

#include <iostream>
using namespace std;

int main()
{
  float a, b, s, p, d, r; //Ввод переменных
  cin >> a;
  cin >> b;
  s = a*a + b*b;// Вычисление арифметических операций
  r = a*a - b*b;
  p = a*a * b*b;
  d = (a*a) / (b*b);
  cout << s <<endl << r <<endl << p <<endl << d;// Вывод результата 
  return 0;
}
