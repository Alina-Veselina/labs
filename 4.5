//Даны два ненулевых числа. Найти сумму, разность, произведение и
//частное их модулей


#include <iostream>
#include <cmath>
using namespace std;

int main()
{
  float a, b, s, p, d, r; //Ввод переменных
  cin >> a;
  cin >> b;
  a = abs(a);// присваивание переменным абсолютные значения
  b = abs(b);
  s = a + b;// Вычисление арифметических операций
  r = a - b;
  p = a * b;
  d = (a) / (b);
  cout << s <<endl << r <<endl << p <<endl << d;// Вывод результата 
  return 0;
}
